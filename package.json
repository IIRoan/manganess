{
  "name": "manganess",
  "main": "expo-router/entry",
  "version": "1.2",
  "expo": {
    "name": "manganess",
    "slug": "manganess",
    "version": "1.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.iroan.manganess",
    "owner": "iroan",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#121212"
      },
      "package": "com.iroan.manganess"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#121212"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "e39fdc53-b1f4-4839-8e48-07a5e789d5eb"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/e39fdc53-b1f4-4839-8e48-07a5e789d5eb",
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "scripts": {
    "start": "expo start",
    "startlocal": "expo start --localhost",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "typecheck": "tsc --noEmit",
    "build": "eas build -p android --profile preview",
    "build:ios": "eas build -p ios --profile preview",
    "build:android": "eas build -p android --profile preview",
    "dev": "expo start --dev-client",
    "prebuild": "expo prebuild --clean",
    "export": "expo export",
    "check": "npm run lint && npm run typecheck && npm run test:ci"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/bottom-sheet": "^5.1.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/slider": "^4.5.7",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@shopify/flash-list": "1.7.6",
    "axios": "^1.7.8",
    "expo": "^53.0.9",
    "expo-blur": "^14.1.5",
    "expo-constants": "~17.1.6",
    "expo-document-picker": "~13.1.5",
    "expo-file-system": "~18.1.9",
    "expo-font": "~13.3.1",
    "expo-haptics": "^14.1.4",
    "expo-linear-gradient": "^14.0.1",
    "expo-router": "~5.0.6",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.8",
    "expo-system-ui": "~5.0.7",
    "expo-updates": "~0.28.12",
    "expo-web-browser": "~14.1.6",
    "html-entities": "^2.5.2",
    "lodash": "^4.17.21",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-color-picker": "^0.6.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/lodash": "^4.17.14",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.3.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~9.2.0",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
  "private": true
}